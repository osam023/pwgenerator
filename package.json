{
    "name": "pwgenerator",
    "displayName": "pwgenerator",
    "description": "Random password generator extension for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "osam023",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.weakPasswordGenerate",
        "onCommand:extension.normalPasswordGenerate",
        "onCommand:extension.strongPasswordGenerate"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.weakPasswordGenerate",
                "title": "Weak password generate."
            },
            {
                "command": "extension.normalPasswordGenerate",
                "title": "Normal password generate."
            },
            {
                "command": "extension.strongPasswordGenerate",
                "title": "Strong password generate."
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
